@model IEnumerable<DoAnChuyenNganh.Models.ThanhVien>

@{
    ViewBag.Title = "Member";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}


<div class="main-page">
    <div class="forms">
        <div class=" form-grids row form-grids-right">
            <div class="widget-shadow " data-example-id="basic-forms">
                <div class="form-title">
                </div>
                
        </div>
    </div>
</div>

<div class="main-page">
    <div class="tables">
        <div class="bs-example widget-shadow" data-example-id="hoverable-table">
            <h4>Thành viên</h4>
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>Mã Thành Viên</th>
                        <th>Tài Khoản</th>
                        <th>Mật Khẩu</th>
                        <th>Họ Tên</th>
                        <th>Địa Chỉ</th>
                        <th>Email</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                    <tr>
                        <td>@Html.DisplayFor(modelItem => item.MaTV)</td>
                        <td>@Html.DisplayFor(modelItem => item.TaiKhoan)</td>
                        <td>@Html.DisplayFor(modelItem => item.MatKhau)</td>
                        <td>@Html.DisplayFor(modelItem => item.HoTen)</td>
                        <td>@Html.DisplayFor(modelItem => item.DiaChi)</td>
                        <td>@Html.DisplayFor(modelItem => item.Email)</td>

                    </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
